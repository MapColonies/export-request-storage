{
  "name": "export-request-storage",
  "version": "1.1.0",
  "description": "handle export statuses with a storage service",
  "main": "index.ts",
  "scripts": {
    "test:unit": "jest --coverage --testPathPattern=\"./src/tests/unit/.*.spec.ts\" --reporters='default' --reporters='jest-html-reporters' --watchAll=false",
    "test:integration": "jest --testPathPattern=\"./src/tests/integration/.*.spec.ts\"",
    "lint": "eslint --ignore-pattern \"src/migration\" \"src/**\" index.ts",
    "lint:fix": "eslint --fix src/**",
    "release": "standard-version",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "build": "tsc --project tsconfig.build.json && copyfiles ./docs ./config dist",
    "start": "npm run build && node ./dist/index.js",
    "confd": "node ./confd/generate-config.js",
    "confd:prod": "node ./confd/generate-config.js --environment production",
    "migration": "node ./src/migration/migration.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "directories": {
    "test": "tests"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@map-colonies/eslint-config": "^1.1.0",
    "@map-colonies/mc-model-types": "^1.0.1",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.8",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "commitlint": "^11.0.0",
    "copyfiles": "^2.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-create-mock-instance": "^1.1.0",
    "jest-html-reporters": "^2.0.4",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@map-colonies/mc-logger": "^1.2.1",
    "@map-colonies/mc-probe": "^1.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "install": "^0.13.0",
    "npm": "^6.14.8",
    "openapi-validator-middleware": "^3.1.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.4",
    "tsyringe": "^4.3.0",
    "typeorm": "0.2.26",
    "yamljs": "^0.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}
